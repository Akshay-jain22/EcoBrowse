

{
  "manifest_version": 2,
  "name": "Data Received Extension",
  "version": "1.0",
  "description": "An extension that logs data received from network requests.",

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],

  "background": {
    "scripts": ["background.js"]
  },
  "browser_action":{
       "default_popup":"./index.html"
  }
}
