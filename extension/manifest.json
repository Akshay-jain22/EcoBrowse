{
	"name" : "Eco-Browse",
	"version" : "2.10.2",
	"manifest_version" : 3,
	"description": "Extension to detect CO2 footprint of a website",
	"background":
	{
		"service_worker": "event.js"
	},
	"permissions": ["tabs","webRequest","declarativeNetRequest","declarativeNetRequestFeedback"],
	"action": {
    "default_title": "This is CO2 detection extension",
    "default_popup": "extension_page.html",
    "default_icon": {
      "32": "icon.png",
      "64": "icon.png",
      "128": "icon.png"
    }
  }
}
